workDir = 'gs://phi-nextflow-bucket/work'

docker.enabled = true

process {
    executor = 'google-batch'
    maxRetries = 3

    publishDir  = [
        path:  { "output/" + task.process },
        mode:"copy",
        enabled: true
    ]
}

google {
    project = 'phinguyen'
    location = 'us-central1'
    batch.spot = true
}